//normal function//
function add(a,b)
{
  return a+b;
}
let c;
c =add(6,9);
console.log("Addition= "=c);
---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
  //Anonymous//
  let add =function(a,b)
  {
    return a+b;
  }
  let c;
  c = a+b;
  console.log("Addition = "+c);
---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
  //Asynochrnonus//
  let add=(a,b) =>
  {
    return a+b;
  }
  let c;
  c = add(6,9);
  console.log("Addition = "+c);
